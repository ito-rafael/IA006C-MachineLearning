\@writefile{toc}{\contentsline {paragraph}{$ i) \tmspace  +\thickmuskip {.2777em} H(X): $ \\ $ \displaystyle H(X) = -\DOTSB \sum@ \slimits@ _{x} p(x) \qopname  \relax o{log}_2 [p(x)] $\\ $ H(X) = -P_X(0) \qopname  \relax o{log}_2 [P_X(0)] -P_X(1) \qopname  \relax o{log}_2 [P_X(1)] $\\ $ \displaystyle H(X) = - \frac  {1}{4} \qopname  \relax o{log}_2 \left ( \frac  {1}{4} \right ) - \frac  {3}{4} \qopname  \relax o{log}_2 \left ( \frac  {3}{4} \right ) $\\ $ H(X) = 0.5 + 0.3113 $\\ $ \unhbox \voidb@x \setbox \@tempboxa \hbox {\color@begingroup \kern \fboxsep {\mathsurround \z@ $\displaystyle  H(X) = 0.8113 $}\kern \fboxsep \color@endgroup }\@tempdima \fboxrule \advance \@tempdima \fboxsep \advance \@tempdima \dp \@tempboxa \hbox {\lower \@tempdima \hbox {\vbox {\hrule height\fboxrule \hbox {\vrule width\fboxrule \relax \vbox {\vskip \fboxsep \box \@tempboxa \vskip \fboxsep }\relax \vrule width\fboxrule }\hrule height\fboxrule }}} $\\ }{3}{section*.9}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{$ ii) \tmspace  +\thickmuskip {.2777em} H(Y): $ \\ $ \displaystyle H(Y) = -\DOTSB \sum@ \slimits@ _{y} p(y) \qopname  \relax o{log}_2 [p(y)] $\\ $ H(Y) = -P_Y(0) \qopname  \relax o{log}_2 [P_Y(0)] -P_Y(1) \qopname  \relax o{log}_2 [P_Y(1)] $\\ $ \displaystyle H(Y) = - \frac  {3}{8} \qopname  \relax o{log}_2 \left ( \frac  {3}{8} \right ) - \frac  {5}{8} \qopname  \relax o{log}_2 \left ( \frac  {5}{8} \right ) $\\ $ H(Y) = 0.5306 + 0.4238 $\\ $ \unhbox \voidb@x \setbox \@tempboxa \hbox {\color@begingroup \kern \fboxsep {\mathsurround \z@ $\displaystyle  H(Y) = 0.9544 $}\kern \fboxsep \color@endgroup }\@tempdima \fboxrule \advance \@tempdima \fboxsep \advance \@tempdima \dp \@tempboxa \hbox {\lower \@tempdima \hbox {\vbox {\hrule height\fboxrule \hbox {\vrule width\fboxrule \relax \vbox {\vskip \fboxsep \box \@tempboxa \vskip \fboxsep }\relax \vrule width\fboxrule }\hrule height\fboxrule }}} $\\ }{4}{section*.10}\protected@file@percent }
